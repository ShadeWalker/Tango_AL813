<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"/>
   <RelativeLayout
        android:id="@+id/linearLayoutMain"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50px"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textView1_skinSat"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_toLeftOf="@+id/seekBar_skinSat"
            android:layout_alignBottom="@+id/seekBar_skinSat" />
        <SeekBar
            android:id="@+id/seekBar_skinSat"
            android:layout_width="280dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_above="@+id/textView_skinSat_progress"
            android:max="100"
            android:progress="50"/>
        <TextView
            android:id="@+id/textView_skinSat"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_toRightOf="@+id/seekBar_skinSat"
            android:layout_alignBottom="@+id/seekBar_skinSat"
             />

        <TextView
            android:id="@+id/textView_skinSat_progress"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true" />

    <RadioGroup
        android:id="@+id/radioGroup1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/textView_skinSat_progress"
        >
        <RadioButton
            android:id="@+id/demooff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:text="@string/m_demo_off"/>

        <RadioButton
            android:id="@+id/vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/m_vertical_demo"/>

        <RadioButton
            android:id="@+id/horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/m_horizontal_demo"/>
    </RadioGroup>
</RelativeLayout>
</RelativeLayout>
